<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
  <!-- A list of agents and their behavior classes -->

  <!-- #### AUCTION AGENTS ### -->
  <agent name="auction-random">
    <set class-path="bin/"/>
    <set class-name="template.AuctionTemplate"/>
  </agent>

  <agent name="auction-km">
    <set class-path="bin/"/>
    <set class-name="template.DummyConstantCostKm"/>
  </agent>

  <agent name="auction-const">
    <set class-path="bin/"/>
    <set class-name="template.DummyConstantBid"/>
  </agent>

  <agent name="auction-repeat">
    <set class-path="bin/"/>
    <set class-name="template.DummyRepeatLast"/>
  </agent>

    <agent name="auction-null">
    <set class-path="bin/"/>
    <set class-name="template.Dummynull"/>
  </agent>

    <agent name="auction-max">
    <set class-path="bin/"/>
    <set class-name="template.DummyMax"/>
  </agent>

  <agent name="auction-own">
    <set class-path="bin/"/>
    <set class-name="template.AuctionAgent"/>
    <set bid-strategy="BEST"/>
  </agent>

</agents>
