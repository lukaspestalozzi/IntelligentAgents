<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveRandom"/>
		<set discount-factor=".85"/>
	</agent>
	
	<agent name="reactive-own">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveAgent"/>
		<set discount-factor=".95"/>
	</agent>
	
		<agent name="reactive-greedy">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveGreedy"/>
		<set discount-factor=".85"/>
	</agent>
	
</agents>
